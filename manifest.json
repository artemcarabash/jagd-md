{
  "manifest_version": 2,
  "name": "Flamedeer Jäger",
  "description": "jägd wie verrückt :D",
  "version": "1.4",
   "background": {
       "scripts": ["background.js"],
      "persistent": true
   },
  "browser_action": {
    "default_icon": "icon-off.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "webNavigation",
    "https://www.mydealz.de/*"]
}